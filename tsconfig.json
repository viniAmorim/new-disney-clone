{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES5",
    "jsx": "react",
    "moduleResolution": "Node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noErrorTruncation": false,
    "allowJs": true
  },
  "exclude": ["node_modules"]
}
